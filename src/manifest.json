{
  "background": {
    "scripts": [
      "./background.js"
    ]
  },
  "browser_action": {
    "default_popup": "./popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "./newtab.html"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "./contentScripts.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description": "vue-chrome-extension-boilerplate",
  "devtools_page": "./devtools.html",
  "homepage_url": "https://www.baidu.com",
  "icons": {
    "16": "assets/icons/icon_16.png",
    "32": "assets/icons/icon_32.png",
    "64": "assets/icons/icon_64.png",
    "128": "assets/icons/icon_128.png"
  },
  "manifest_version": 2,
  "name": "自定义插件",
  "options_ui": {
    "chrome_style": true,
    "page": "./options.html"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*",
    "fontSettings",
    "<all_urls>",
    "bookmarks"
  ],
  "version": "1.0.0",
  "web_accessible_resources": [
    "js/inject.js"
  ]
}